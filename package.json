{
  "name": "alreadydone",
  "version": "0.0.0",
  "description": "A simplistic, collaborative realtime todo web app that works offline.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:debug": "echo 'Use `debugger` in code to set a breakpoint.' && node_modules/.bin/devtool node_modules/mocha/bin/_mocha",
    "test-containerised": "docker-compose -f docker-compose-dev.yml run web npm run test",
    "test-containerised:watch": "docker-compose -f docker-compose-dev.yml run web npm run test:watch",
    "test:coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "build": "browserify js/app.js -o js/bundle.js",
    "watch": "watchify --debug js/app.js -o js/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pixelistik/alreadydone.git"
  },
  "keywords": [
    "todo"
  ],
  "author": "pixelistik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pixelistik/alreadydone/issues"
  },
  "dependencies": {
    "knockout": "~3.4.0",
    "lodash": "~3.10.1",
    "express": "~4.13.3",
    "browserify": "~12.0.2",
    "bootstrap": "~3.3.6",
    "localforage": "~1.3.1",
    "uuid": "~2.0.1",
    "npm-zepto": "~1.1.7",
    "mongodb": "~2.1.4",
    "body-parser": "~1.14.2",
    "sortablejs": "~1.4.2"
  },
  "devDependencies": {
    "watchify": "~3.6.1",
    "eslint": "~1.10.3",
    "karma": "~0.13.19",
    "mocha": "~2.3.4",
    "chai": "~3.4.1",
    "sinon": "~1.17.2",
    "supertest": "~1.1.0",
    "istanbul": "~0.4.2",
    "devtool": "~1.8.3"
  }
}
